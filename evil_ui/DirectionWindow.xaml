<Window x:Name="DirectionWin" x:Class="evil_ui.DirectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:evil_ui"
        mc:Ignorable="d"
        Title="Directions" Height="1348" Width="1310" ResizeMode="NoResize"
        Loaded="Images">
    <Grid>
        <Image x:Name="Direction_Img" Margin="0,0,0,0" Height="1298" Width="1310" Cursor="None"/>
        <Button Content="Close" Margin="0,575,0,0" Height="50" Width="100" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
